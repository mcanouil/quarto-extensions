  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "lint": "eslint",
    "watch": "tsc -watch -p ./"
  },
  "devDependencies": {
    "@types/node": "^22.8.6",
    "@types/vscode": "^1.75.0",
    "typescript": "^5.6.2"
  }
}
